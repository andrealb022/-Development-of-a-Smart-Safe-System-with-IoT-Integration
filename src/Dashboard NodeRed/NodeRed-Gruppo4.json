        "format": "<table>\n    <thead>\n        <tr>\n            <th>Motivazione</th>\n            <th>Data e Ora</th>\n        </tr>\n    </thead>\n    <tbody id=\"table-body\">\n    </tbody>\n</table>\n\n<script>\n    // Funzione per aggiungere una nuova riga alla tabella\n  function addTableRow(motivazione, dataOra) {\n    var tableBody = document.getElementById(\"table-body\");\n    \n    var rows = tableBody.getElementsByTagName(\"tr\");\n    for (var i = 0; i < rows.length; i++) { \n      var row=rows[i];\n      var dateCell=row.getElementsByTagName(\"td\")[1];\n      if (dateCell.textContent===dataOra) { // Esiste già una riga con la stessa data, non aggiungere una nuova riga \n        return; }\n      }\n    \n    var newRow = document.createElement(\"tr\");\n    var motivazioneCell = document.createElement(\"td\");\n    var dataOraCell = document.createElement(\"td\");\n \n    motivazioneCell.textContent = motivazione;\n    dataOraCell.textContent = dataOra;\n \n    newRow.appendChild(motivazioneCell);\n    newRow.appendChild(dataOraCell);\n    tableBody.appendChild(newRow);\n  }\n \n  // Recupera i dati salvati dal localStorage e ricostruisci la tabella\n  function restoreTableData() {\n    var tableBody = document.getElementById(\"table-body\");\n    var savedData = localStorage.getItem(\"tableData\");\n \n    if (savedData) {\n      var data = JSON.parse(savedData);\n \n      tableBody.innerHTML = \"\";\n\n      data.forEach(function(row) {\n        addTableRow(row.motivazione, row.dataOra);\n      });\n    }\n  }\n \n  // Salva i dati della tabella nel localStorage\n  function saveTableData(motivazione, dataOra) {\n    var savedData = localStorage.getItem(\"tableData\");\n    var data = [];\n \n    if (savedData) {\n      data = JSON.parse(savedData);\n    }\n \n    data.push({ motivazione: motivazione, dataOra: dataOra });\n \n    localStorage.setItem(\"tableData\", JSON.stringify(data));\n  }\n \n  // Registra la funzione di gestione dell'evento\n  (function(scope) {\n    scope.$watch('msg', function(msg) {\n      if (msg) {\n          var motivazione = msg.payload.motivazione;\n          var dataOra = msg.payload.data;\n          addTableRow(motivazione, dataOra);\n          saveTableData(motivazione, dataOra);\n      }\n    });\n \n    // Richiama la funzione per ripristinare i dati salvati\n    restoreTableData();\n  })(scope);\n</script>",
